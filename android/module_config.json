{
  "author": "jamesfchen",
  "description": "该配置文件给idea plugin插件和gradle plugin读取,idea plugin通过该文件可以实现管理想要编译的模块，gradle plugin则可以根据这些模块信息生成快速模块",
  "buildVariants": [
    "debug",
    "release"
  ],
  "groupId": "io.github.jamesfchen",
  "allModules": [
    {
      "simpleName": "app",
      "format": "app",
      "group": "father",
      "sourcePath": ":app"
    },
    {
      "simpleName": "update-main",
      "format": "app",
      "group": "update",
      "sourcePath": ":components:update-module:main"
    },
    {
      "simpleName": "update-appupdate",
      "format": "bundle",
      "group": "update",
      "sourcePath": ":components:update-module:appupdate"
    },
    {
      "simpleName": "codescanner-main",
      "format": "app",
      "group": "codescanner",
      "sourcePath": ":components:codescanner-module:main"
    },
    {
      "simpleName": "codescanner-qrcode",
      "projectDir": "",
      "format": "bundle",
      "group": "codescanner",
      "sourcePath": ":components:codescanner-module:qrcode"
    },
    {
      "simpleName": "im-main",
      "format": "app",
      "group": "im",
      "sourcePath": ":components:im-module:main"
    },
    {
      "simpleName": "im-chatroom",
      "format": "bundle",
      "group": "im",
      "sourcePath": ":components:im-module:chatroom"
    },
    {
      "simpleName": "account-main",
      "format": "app",
      "group": "account",
      "sourcePath": ":components:account-module:main"
    },
    {
      "simpleName": "account-rnplugin",
      "format": "bundle_foundation",
      "group": "account",
      "sourcePath": ":components:account-module:rnplugin"
    },
    {
      "simpleName": "account-foundation",
      "format": "bundle_foundation",
      "group": "account",
      "sourcePath": ":components:account-module:foundation"
    },
    {
      "simpleName": "account-impl",
      "format": "bundle",
      "group": "account",
      "sourcePath": ":components:account-module:impl",
      "deps": [
        "framework-loader",
        "account-export"
      ]
    },
    {
      "simpleName": "account-export",
      "format": "bundle_foundation",
      "group": "account",
      "sourcePath": ":components:account-module:export"
    },
    {
      "simpleName": "home-main",
      "format": "app",
      "group": "home",
      "sourcePath": ":components:home-module:main"
    },
    {
      "simpleName": "home-myhome",
      "format": "bundle",
      "group": "home",
      "sourcePath": ":components:home-module:myhome"
    },
    {
      "simpleName": "hotel-main",
      "format": "app",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:main"
    },
    {
      "simpleName": "hotel-bundle1",
      "format": "bundle",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:bundle1"
    },
    {
      "simpleName": "hotel-bundle2",
      "format": "bundle",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:bundle2"
    },
    {
      "simpleName": "hotel-lint",
      "format": "jar",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:hotel-lint"
    },
    {
      "simpleName": "hotel-foundation",
      "format": "bundle_foundation",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:foundation"
    },
    {
      "simpleName": "box-main",
      "format": "app",
      "group": "box",
      "sourcePath": ":components:box-module:main"
    },
    {
      "simpleName": "box-tool",
      "format": "bundle",
      "group": "box",
      "sourcePath": ":components:box-module:tool"
    },
    {
      "simpleName": "framework-loader",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:loader",
      "type": "api"
    },
    {
      "simpleName": "framework-baseui",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:baseui",
      "type": "api"
    },
    {
      "simpleName": "framework-network",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:network",
      "type": "api"
    },
    {
      "simpleName": "framework-av",
      "group": "fwk",
      "format": "framework_foundation",
      "sourcePath": ":framework:av",
      "type": "api"
    },
    {
      "simpleName": "framework-image",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:image",
      "type": "api"
    },
    {
      "simpleName": "framework-mockserver",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:mockserver",
      "type": "api"
    },
    {
      "simpleName": "framework-storage",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:storage",
      "type": "api"
    },
    {
      "simpleName": "framework-common",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":framework:common",
      "type": "api"
    }
  ]
}
