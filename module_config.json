{
  "author": "electrolytej",
  "description": "该配置文件给idea plugin插件和gradle plugin读取,idea plugin通过该文件可以实现管理想要编译的模块，gradle plugin则可以根据这些模块信息生成快速模块",
  "buildVariants": [
    "tvDebug",
    "tvRelease"
  ],
  "groupId": "io.github.electrolytej",
  "allModules": [
    {
      "simpleName": "app",
      "sourcePath": ":app",
      "group": "host"
    },
    {
      "simpleName": "framework-lint-rules",
      "group": "fwk",
      "sourcePath": ":lint-rules"
    },
    {
      "simpleName": "framework-base",
      "group": "fwk",
      "sourcePath": ":base"
    },
    {
      "simpleName": "uamp-uamp",
      "format": "nsbundle",
      "group": "uamp",
      "sourcePath": ":modules:uamp-module:uamp"
    },
    {
      "simpleName": "update-bundle",
      "format": "nsbundle",
      "group": "update",
      "sourcePath": ":modules:update-bundle"
    },
    {
      "simpleName": "codescanner-bundle",
      "format": "nsbundle",
      "group": "codescanner",
      "sourcePath": ":modules:codescanner-bundle"
    },
    {
      "simpleName": "im-bundle",
      "format": "nsbundle",
      "group": "im",
      "sourcePath": ":modules:im-bundle"
    },
    {
      "simpleName": "im-export",
      "format": "api",
      "group": "im",
      "sourcePath": ":modules:im-export"
    },
    {
      "simpleName": "account-bundle",
      "format": "nsbundle",
      "group": "account",
      "sourcePath": ":modules:account-bundle"
    },
    {
      "simpleName": "main-bundle",
      "format": "nsbundle",
      "group": "main",
      "sourcePath": ":modules:main-bundle"
    },
    {
      "simpleName": "main-export",
      "format": "api",
      "group": "main",
      "sourcePath": ":modules:main-export"
    },
    {
      "simpleName": "hotel-bundle1",
      "format": "ndbundle",
      "group": "hotel",
      "sourcePath": ":modules:hotel_bundle1"
    },
    {
      "simpleName": "hotel-bundle2",
      "format": "nsbundle",
      "group": "hotel",
      "sourcePath": ":modules:hotel-bundle2"
    },
    {
      "simpleName": "hotel-export",
      "format": "api",
      "group": "hotel",
      "sourcePath": ":modules:hotel-export"
    },
    {
      "simpleName": "hotel-foundation",
      "group": "hotel",
      "sourcePath": ":modules:hotel-foundation"
    },
    {
      "simpleName": "hotel-tool",
      "group": "hotel",
      "sourcePath": ":modules:hotel-tool"
    },
    {
      "simpleName": "feeds-bundle",
      "format": "nsbundle",
      "group": "feeds",
      "sourcePath": ":modules:feeds-bundle"
    },
    {
      "simpleName": "livestreaming-bundle",
      "format": "nsbundle",
      "group": "livestreaming",
      "sourcePath": ":modules:livestreaming-bundle"
    },
    {
      "simpleName": "ad-bundle",
      "format": "nsbundle",
      "group": "commerce",
      "sourcePath": ":modules:ad-bundle"
    },
    {
      "simpleName": "ad-export",
      "format": "api",
      "group": "commerce",
      "sourcePath": ":modules:ad-export"
    }
  ]
}
