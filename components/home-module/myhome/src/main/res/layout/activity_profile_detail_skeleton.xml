<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f6"
    tools:background="@android:color/black">

    <ImageView
        android:id="@+id/id_first_frame"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/iv_reflection"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/iv_reflection"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_first_frame" />


    <View
        android:id="@+id/view1"
        android:layout_width="198dp"
        android:layout_height="16dp"
        android:layout_marginStart="24dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_first_frame" />

    <View
        android:id="@+id/view2"
        android:layout_width="140dp"
        android:layout_height="12dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view1" />

    <TextView
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="24dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/view4"
        android:layout_width="53dp"
        android:layout_height="12dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="24dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintBaseline_toBaselineOf="@+id/view3"
        app:layout_constraintRight_toRightOf="parent" />

    <View
        android:id="@+id/divider1"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="9.5dip"
        android:background="#eeeeee"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <TextView
        android:id="@+id/view5"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginTop="12.5dp"
        android:layout_marginEnd="24dip"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/divider1" />

    <TextView
        android:id="@+id/view6"
        android:layout_width="53dp"
        android:layout_height="12dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintBaseline_toBaselineOf="@+id/view5"
        app:layout_constraintRight_toRightOf="@+id/view4" />

    <View
        android:id="@+id/divider2"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="14dp"
        android:background="#eeeeee"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <!--    <View-->
    <!--        android:id="@+id/address_module_shadow_back"-->
    <!--        android:layout_width="match_parent"-->
    <!--        android:layout_height="63dip"-->
    <!--        android:layout_marginLeft="8dip"-->
    <!--        android:layout_marginRight="8dip"-->
    <!--        android:background="@drawable/bg_shadow"-->
    <!--        app:layout_constraintEnd_toStartOf="@+id/view6"-->
    <!--        app:layout_constraintStart_toStartOf="@+id/view3"-->
    <!--        app:layout_constraintTop_toBottomOf="@+id/divider2" />-->

    <TextView
        android:id="@+id/view7"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginTop="12.5dp"
        android:layout_marginEnd="24dip"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/divider2" />

    <ImageView
        android:id="@+id/view8"
        android:layout_width="53dp"
        android:layout_height="12dp"
        android:src="@mipmap/ic_launcher"
        android:tag="breathing"
        app:layout_constraintRight_toRightOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <TextView
        android:id="@+id/view9"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="24dip"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/view10"
        android:layout_width="53dp"
        android:layout_height="12dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintBaseline_toBaselineOf="@+id/view9"
        app:layout_constraintRight_toRightOf="@+id/view4" />

    <View
        android:id="@+id/view11"
        android:layout_width="235dp"
        android:layout_height="20dp"
        android:layout_marginTop="40dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <View
        android:id="@+id/view12"
        android:layout_width="36dp"
        android:layout_height="20dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintRight_toRightOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <View
        android:id="@+id/divider3"
        android:layout_width="0dp"
        android:layout_height="2px"
        android:layout_marginTop="12dp"
        android:background="#eeeeee"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view11" />

    <View
        android:id="@+id/view13"
        android:layout_width="235dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dip"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/divider3" />

    <View
        android:id="@+id/view14"
        android:layout_width="75dp"
        android:layout_height="20dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintRight_toRightOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view13" />

    <View
        android:id="@+id/view15"
        android:layout_width="64dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view16"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view13" />

    <View
        android:id="@+id/view16"
        android:layout_width="64dp"
        android:layout_height="30dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view17"
        app:layout_constraintStart_toEndOf="@+id/view15"
        app:layout_constraintTop_toTopOf="@+id/view15" />

    <View
        android:id="@+id/view17"
        android:layout_width="64dp"
        android:layout_height="30dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toStartOf="@+id/view18"
        app:layout_constraintStart_toEndOf="@+id/view16"
        app:layout_constraintTop_toTopOf="@+id/view15" />

    <View
        android:id="@+id/view18"
        android:layout_width="64dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toStartOf="@+id/view19"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintStart_toEndOf="@+id/view17"
        app:layout_constraintTop_toTopOf="@+id/view15" />

    <View
        android:id="@+id/view19"
        android:layout_width="64dp"
        android:layout_height="30dp"
        android:background="#f0f2f5"
        android:tag="breathing"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintStart_toEndOf="@+id/view18"
        app:layout_constraintTop_toTopOf="@+id/view15" />
</androidx.constraintlayout.widget.ConstraintLayout>